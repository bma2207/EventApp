<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="event" package="com.axelor.event.db" />

	<entity name="Address" cachable="true">
		<integer name="beforeDays"  title="Before Days"/>
		<decimal name="discountPercent" title="Discount Percent"/>
		<decimal name="discountAmount"  title="Discount Amount"/>
		<string name="flatHouseNo" title="Flat/House No"/>
		<string name="street" title="Street" namecolumn="true"/>
		<string name="landMark" title="LandMark"/>
		<string name="city" title="City"/>
		<string name="country" title="Country"/>
		<string name="fullName" title="Full Name"/>
	</entity>
	
</domain-models>